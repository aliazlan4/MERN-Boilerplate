{
  "name": "mern-boilerplate",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "engines": {
    "node": ">=12.0.0",
    "yarn": ">=1.15.*"
  },
  "scripts": {
    "start": "cd build-server && node server.js",
    "start-client": "react-scripts start",
    "build-client": "react-scripts build",
    "test-client": "react-scripts test --coverage --watchAll=false",
    "start-server": "nodemon --watch server --exec babel-node server/server.js",
    "test-server": "jest --config=server/jest.config.js /server --coverage",
    "build-server": "babel ./server --out-dir build-server --ignore \"server/coverage\",\"server/**/*.test.js\",\"server/jest.config.js\" --presets minify",
    "lint": "eslint . --ignore-path .gitignore",
    "seed": "babel-node tools/seedDatabase.js"
  },
  "dependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "antd": "^4.3.4",
    "axios": "^0.19.2",
    "babel-preset-minify": "^0.5.1",
    "bcrypt": "^5.0.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongoose": "^5.9.19",
    "node-sass": "^7.0.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "validatorjs": "^3.18.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^10.4.1",
    "axios-mock-adapter": "^1.18.1",
    "chalk": "^4.1.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "mongodb-memory-server": "^6.6.1",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
